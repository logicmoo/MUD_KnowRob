	screla:hasBaseUnit units:centimeter;
	mrela:toThePower "3"^^xsd:double;
	rdfs:subClassOf [
		a owl:Restriction; owl:onProperty quant:unitOf; owl:someValuesFrom quant:VolumeMeasurement];
	screla:hasSymbol "cm³", "cc";
	skos:exactMatch [
		a skos:Concept; skos:inScheme clisko:sct20080731; skos:notation "258773002"^^clisko:sct20080731DT].

